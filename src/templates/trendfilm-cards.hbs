
{{#each this}}

<li class="list__item">
 
  <img 
  id="{{id}}"
  class="item__img"
  src='https://image.tmdb.org/t/p/w500/{{poster_path}}'
  alt="{{title}}"/>

  <div class="item__wrapper">
      <h2 class="item__title">{{title}}</h2>
   <div class="about-movie">
         <p class="irem__text">{{genre_ids}}</p>
         <p class="irem__text"> | {{release_date}}</p>
         <p>{{vote_average}}</p>
   </div>
  </div>
</li>

{{/each}}
